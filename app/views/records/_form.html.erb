<%= render "layouts/errors", :object => @record %>

<%= form_for @record do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br><br>
  <%= f.label :artist %><br>
  <%= f.text_field :artist %><br><br>
  <%= f.label :price %><br>
  $<%= f.number_field :price %>.00<br><br>
  <%= f.label :description %><br>
  <%= f.text_area :description %><br><br>
  <%= f.label :genre %><br>
  <%= f.text_field :genre %><br><br>
  <%= f.label :album_length %><br>
  <%= f.number_field :album_length %> minutes
  <br><br>
  <%= f.submit class: "new"  %>
  <%= link_to "Back", :back %>
<% end %>
